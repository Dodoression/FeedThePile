; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Feed The Pile!"
run/main_scene="res://Scene/main_menu.tscn"
config/features=PackedStringArray("4.2", "Mobile")
boot_splash/image="res://Sprites/intro.png"
config/icon="res://icon.svg"

[autoload]

GameManager="*res://Scene/GameManager.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=320
window/size/resizable=false
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Feed The Pile!"

[input]

leftArrow={
"deadzone": 0.5,
"events": []
}
rightArrow={
"deadzone": 0.5,
"events": []
}
upArrow={
"deadzone": 0.5,
"events": []
}
sock={
"deadzone": 0.5,
"events": []
}

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/emulate_mouse_from_touch=false

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
